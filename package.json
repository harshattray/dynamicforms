    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "lodash": "^4.17.4",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "react-redux-form": "^1.16.10",
