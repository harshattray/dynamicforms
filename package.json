    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0-beta.6",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-to-json": "^3.3.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^23.6.0",
